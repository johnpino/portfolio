{
	"extends": ["next/core-web-vitals", "eslint:recommended", "airbnb", "prettier"],
	"plugins": ["prettier"],
	"root": true,
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"semi": false
			}
		],
		"react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }]
	}
}
